    "import numpy as np\n",
    "import ast\n",
    "\n",
    "input_list = ast.literal_eval(input())\n",
    "m = int(input())\n",
    "n = int(input())\n",
    "\n",
    "array_1 = np.array(input_list)\n",
    "\n",
    "final_array = array_1[ (array_1 > m) & (array_1 <n)]\n",
    "print(final_array)"
   ]
