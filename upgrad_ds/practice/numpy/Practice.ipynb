    "# Another Solution\n",
    "\n",
    "import numpy as np\n",
    "import ast\n",
    "\n",
    "input_list = ast.literal_eval(input())\n",
    "m = int(input())\n",
    "n = int(input())\n",
    "\n",
    "array_1 = np.array(input_list)\n",
    "\n",
    "print(array_1[np.logical_and(array_1>m, array_1<n)])"
   ]
